set.seed(1)
play <- function() {
my_money <- 1
i <- 0
while(my_money<2&&my_money>-1) {
i <- i+1
#cat(i, " ", my_money, "\n")
d <- runif(1)
#cat(i, ",", my_money, ",", d, "\n")
if (d<0.354)
my_money <- my_money+1
else if (d>=0.654)
my_money <-  my_money-1
}
return(list(nturns=i, winnings=my_money))
}
play()
